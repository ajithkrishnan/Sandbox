# to generate ssh key: ssh-keygen -t rsa 
# copy this file to ~/.ssh/config
Host gitlab git.rwth-aachen.de 
    Hostname git.rwth-aachen.de 
    IdentityFile ~/.ssh/id_rsa_gitlab
    User krishnan

Host github github.com
    Hostname github.com
    IdentityFile ~/.ssh/id_rsa_github
    User ajithkrishnan
